<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" maxHeight="583.0" prefHeight="459.0" prefWidth="804.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.UserAllIssuedBooksController">
    <children>
        <TableView fx:id="issuedBookstable" layoutX="147.0" layoutY="25.0" prefHeight="482.0" prefWidth="657.0">
            <columns>
                <TableColumn fx:id="titleColumn" prefWidth="221.0" text="Title" />
                <TableColumn fx:id="ISBNColumn" minWidth="0.0" prefWidth="145.0" text="ISBN" />
                <TableColumn fx:id="authorColumn" minWidth="0.0" prefWidth="162.0" text="Author" />
                <TableColumn fx:id="publisherColumn" minWidth="0.0" prefWidth="146.0" text="Publisher" />
            </columns>
        </TableView>
        <Button layoutX="6.0" layoutY="394.0" mnemonicParsing="false" onMouseClicked="#backAction" prefHeight="45.0" prefWidth="131.0" text="Exit" textFill="#3f2b63">
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
        </Button>
        <Button layoutX="6.0" layoutY="25.0" mnemonicParsing="false" onAction="#issueBookAction" prefHeight="45.0" prefWidth="131.0" text="Issue book" textFill="#3f2b63">
            <font>
                <Font name="System Bold" size="17.0" />
            </font>
        </Button>
        <Button layoutX="6.0" layoutY="81.0" mnemonicParsing="false" onMouseClicked="#returnbookAction" prefHeight="45.0" prefWidth="131.0" text="Return book" textFill="#3f2b63">
            <font>
                <Font name="System Bold" size="17.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
